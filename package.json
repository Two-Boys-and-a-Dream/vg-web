{
    "name": "vg-web",
    "version": "1.0.0",
    "description": "Video Game Website",
    "source": "src/index.html",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "scripts": {
        "start": "parcel --open",
        "clear-cache": "rmdir /s .parcel-cache",
        "clean": "rmdir /s dist",
        "clean-project": "rmdir /s node_modules && rmdir /s .parcel-cache && npm install",
        "build": "parcel build --public-url ./",
        "test": "jest",
        "lint": "eslint .",
        "pretty": "prettier --write .",
        "pretty-check": "prettier --check .",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Two-Boys-and-a-Dream/vg-web.git"
    },
    "keywords": [
        "React",
        "Video Games"
    ],
    "author": "Raymond-Cox",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Two-Boys-and-a-Dream/vg-web/issues"
    },
    "homepage": "https://github.com/Two-Boys-and-a-Dream/vg-web#readme",
    "dependencies": {
        "@chakra-ui/react": "^2.4.4",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "axios": "0.27.2",
        "framer-motion": "^7.10.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-query": "^3.39.2",
        "react-router-dom": "^6.6.1"
    },
    "devDependencies": {
        "@babel/core": "^7.20.5",
        "@babel/preset-react": "^7.18.6",
        "@parcel/babel-plugin-transform-runtime": "^2.8.1",
        "@parcel/babel-preset-env": "^2.8.1",
        "@parcel/transformer-sass": "^2.8.2",
        "@tanstack/eslint-plugin-query": "^4.20.8",
        "@testing-library/react": "^13.4.0",
        "@types/jest": "^29.2.4",
        "babel-jest": "^29.3.1",
        "buffer": "^5.7.1",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^27.1.7",
        "eslint-plugin-n": "^15.6.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.31.11",
        "husky": "^8.0.2",
        "jest": "^29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "jest-sonar": "^0.2.15",
        "jest-transform-stub": "^2.0.0",
        "lint-staged": "^13.1.0",
        "parcel": "^2.8.2",
        "prettier": "2.8.1",
        "process": "^0.11.10"
    },
    "lint-staged": {
        "*.js": "eslint .",
        "*.{js,css,scss,md,json}": "prettier --write"
    }
}
